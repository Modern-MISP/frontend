{
  "name": "misp-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start:misp": "docker compose -f misp-docker/docker-compose.yml up",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test:open": "cypress open",
    "lint": "prettier --check . '!misp-docker/**' && eslint .",
    "format": "prettier --write . '!misp-docker/**'",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "gen:api-types": "openapi-typescript https://raw.githubusercontent.com/MISP/MISP/develop/app/webroot/doc/openapi.yaml -o src/lib/api/misp.d.ts"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.6",
    "@dagrejs/dagre": "^1.0.4",
    "@histoire/plugin-svelte": "^0.17.9",
    "@iconify/svelte": "^3.1.6",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.72.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/svelte": "^4.1.0",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.3.0",
    "@xyflow/svelte": "^0.0.35",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.4",
    "date-fns": "^3.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "histoire": "^0.17.9",
    "jsdom": "^24.0.0",
    "lodash-es": "^4.17.21",
    "openapi-fetch": "^0.8.2",
    "openapi-typescript": "^6.7.4",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "ts-extras": "^0.11.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vitest": "^1.2.1"
  },
  "type": "module"
}
